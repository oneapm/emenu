<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hibernate.list.List1" table="list" catalog="e_menu">
        <id name="id" type="java.lang.Integer">
            <column name="Id" />
            <generator class="identity" />
        </id>
        <property name="listOrdNum" type="java.lang.Integer">
            <column name="list_ord_num">
                <comment>订单号</comment>
            </column>
        </property>
        <property name="listMeNum" type="java.lang.Integer">
            <column name="list_me_num">
                <comment>菜编号</comment>
            </column>
        </property>
        <property name="listPrise" type="java.lang.String">
            <column name="list_prise">
                <comment>订单上的价格</comment>
            </column>
        </property>
        <property name="listResNum" type="java.lang.String">
            <column name="list_res_num">
                <comment>饭店编号</comment>
            </column>
        </property>
        <property name="listUserName" type="java.lang.String">
            <column name="list_user_name">
                <comment>用户名</comment>
            </column>
        </property>
        <property name="addperson" type="java.lang.String">
            <column name="addperson">
                <comment>添加人</comment>
            </column>
        </property>
        <property name="addtime" type="java.lang.String">
            <column name="addtime">
                <comment>添加时间</comment>
            </column>
        </property>
        <property name="updateperson" type="java.lang.String">
            <column name="updateperson">
                <comment>更新人</comment>
            </column>
        </property>
        <property name="updatetime" type="java.lang.String">
            <column name="updatetime">
                <comment>更新时间</comment>
            </column>
        </property>
        <property name="deletesign" type="java.lang.Integer">
            <column name="deletesign">
                <comment>删除标识</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
